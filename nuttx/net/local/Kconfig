menu "Unix Domain Socket Support"
	depends on NET

config NET_LOCAL
	bool "Unix domain (local) sockets"
	default n
	---help---
		Enable or disable Unix domain (aka Local) sockets.

if NET_LOCAL

endif # NET_LOCAL

endmenu # Unix Domain Sockets
